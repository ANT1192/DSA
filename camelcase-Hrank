int camelcase(string s) {
    int cnt=1;
    cout<< (int) 'a'<< ":"<<(int) 'z';// prints range of 'a' to 'z' 97:122
    cout<<endl;
    cout<< (int) 'A'<< ":"<<(int) 'Z';//prints range of 'A' to 'Z' 65:90
    for(int i=0;i<s.length();i++)
    {
        if(s[i]<'a')
        cnt++;
    }
 return cnt;
}
