#include <bits/stdc++.h>

using namespace std;

string ltrim(const string &);
string rtrim(const string &);
vector<string> split(const string &);

/*
 * Complete the 'plusMinus' function below.
 *
 * The function accepts INTEGER_ARRAY arr as parameter.
 */

void plusMinus(vector<int> arr) {
double Pcnt=0;
double Ncnt=0;
double Ocnt=0;
for(int i=0;i<arr.size();i++)
{
    if(arr[i]<0)
    {
        Ncnt++;
    }
    else if(arr[i]==0)
    {
        Ocnt++;
    }
    else if(arr[i]>0)
    {
        Pcnt++;
    }
}
    double PropOccP= Pcnt/arr.size();
    double PropOccN= Ncnt/arr.size();
    double PropOccO= Ocnt/arr.size();
    
    
  cout<<setprecision(6)<<fixed;
  cout <<PropOccP<< endl;
  cout <<PropOccN<< endl;
  cout <<PropOccO<< endl;
}
